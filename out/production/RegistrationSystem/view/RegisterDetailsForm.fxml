<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="registrationDetailContext" prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegisterDetailsFormController">
   <children>
      <Label alignment="CENTER" prefHeight="45.0" prefWidth="700.0" text="Registration Details" textFill="#032668">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="13.0" layoutY="11.0" onMouseClicked="#moveToHome" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/home.png" />
         </image>
      </ImageView>
      <TableView fx:id="tblRegistrationDetails" layoutX="34.0" layoutY="245.0" prefHeight="238.0" prefWidth="632.0">
        <columns>
          <TableColumn fx:id="colRegId" prefWidth="105.0" text="RegId" />
          <TableColumn fx:id="colSId" prefWidth="105.0" text="StudentId" />
            <TableColumn fx:id="colSName" prefWidth="105.0" text="Name" />
            <TableColumn fx:id="colCId" prefWidth="105.0" text="CourseId" />
            <TableColumn fx:id="colCName" prefWidth="105.0" text="Course Name" />
            <TableColumn fx:id="colRegDate" prefWidth="105.0" text="RegDate" />
        </columns>
      </TableView>
      <Button layoutX="504.0" layoutY="203.0" mnemonicParsing="false" onAction="#searchOnAction" prefHeight="25.0" prefWidth="69.0" styleClass="searchBtn" stylesheets="@assets/css/styles.css" text="Search" />
      <JFXTextField fx:id="txtSearchBar" layoutX="37.0" layoutY="203.0" prefHeight="25.0" prefWidth="452.0" styleClass="textType" stylesheets="@assets/css/styles.css" />
      <JFXTextField fx:id="txtSName" labelFloat="true" layoutX="262.0" layoutY="100.0" prefHeight="29.0" prefWidth="172.0" promptText="Student Name : " styleClass="textType" stylesheets="@assets/css/styles.css">
         <font>
            <Font name="Ubuntu" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtSId" labelFloat="true" layoutX="76.0" layoutY="100.0" onKeyReleased="#keyReleasedOnAction" prefHeight="29.0" prefWidth="172.0" promptText="Id : " styleClass="textType" stylesheets="@assets/css/styles.css">
         <font>
            <Font name="Ubuntu" size="13.0" />
         </font>
      </JFXTextField>
      <Label layoutX="50.0" layoutY="64.0" prefHeight="17.0" prefWidth="77.0" styleClass="textType" stylesheets="@assets/css/styles.css" text="Register ID : ">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label fx:id="lblRegId" layoutX="127.0" layoutY="64.0" prefHeight="17.0" prefWidth="77.0" styleClass="textType" stylesheets="@assets/css/styles.css" text="R001">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtCourseId" labelFloat="true" layoutX="76.0" layoutY="153.0" prefHeight="29.0" prefWidth="155.0" promptText="Course Id : " styleClass="textType" stylesheets="@assets/css/styles.css">
         <font>
            <Font name="Ubuntu" size="13.0" />
         </font>
      </JFXTextField>
      <JFXComboBox fx:id="cmbCourse" labelFloat="true" layoutX="452.0" layoutY="101.0" prefHeight="28.0" prefWidth="172.0" promptText="Course  :" styleClass="combo" stylesheets="@assets/css/styles.css" />
      <JFXTextField fx:id="txtFee" labelFloat="true" layoutX="242.0" layoutY="152.0" prefHeight="29.0" prefWidth="155.0" promptText="Fee : " styleClass="textType" stylesheets="@assets/css/styles.css">
         <font>
            <Font name="Ubuntu" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtDuration" labelFloat="true" layoutX="409.0" layoutY="151.0" prefHeight="29.0" prefWidth="155.0" promptText="Duration : " styleClass="textType" stylesheets="@assets/css/styles.css">
         <font>
            <Font name="Ubuntu" size="13.0" />
         </font>
      </JFXTextField>
      <Button fx:id="regBtn" layoutX="597.0" layoutY="203.0" mnemonicParsing="false" onAction="#registerOnAction" prefHeight="25.0" prefWidth="69.0" styleClass="updateBtn" stylesheets="@assets/css/styles.css" text="Register" />
      <CheckBox fx:id="checkBox" layoutX="576.0" layoutY="163.0" mnemonicParsing="false" onAction="#paidOnAction" styleClass="textType" stylesheets="@assets/css/styles.css" text="Paid" />
   </children>
</AnchorPane>
